{
  "name": "twi-ext",
  "version": "1.0.0",
  "description": "Unofficial utility to develop browser extensions for Twitter",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx ts-auto-guard \"./src/types/internal.ts\" && npx webpack && npx banner-cli ./dist/**/*.js && npx banner-cli ./dist/**/*.ts",
    "prepublishOnly": "npm run build",
    "doc": "npx typedoc --plugin typedoc-plugin-markdown --cleanOutputDir --excludePrivate --includeVersion --readme none \"src/index.ts\"",
    "version": "npm run build && npm run doc && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robot-Inventor/twi-ext.git"
  },
  "keywords": [
    "twitter",
    "extension",
    "web extension",
    "utility"
  ],
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "author": "Robot-Inventor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robot-Inventor/twi-ext/issues"
  },
  "homepage": "https://github.com/Robot-Inventor/twi-ext#readme",
  "devDependencies": {
    "banner-cli": "^0.14.3",
    "ts-auto-guard": "^4.1.2",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.26",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  }
}